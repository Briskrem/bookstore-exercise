{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "books"
    ],
    "properties": {
        "books": {
            "type": "object",
            "default": {},
            "title": "The books Schema",
            "required": [
                "isbn",
                "amazon_url",
                "author",
                "language",
                "pages",
                "publisher",
                "title",
                "year"
            ],
            "properties": {
                "isbn": {
                    "type": "string",
                    "default": "",
                    "title": "The isbn Schema",
                    "examples": [
                        "numbers as text"
                    ]
                },
                "amazon_url": {
                    "type": "string",
                    "default": "",
                    "title": "The amazon_url Schema",
                    "examples": [
                        "i hv no idk"
                    ]
                },
                "author": {
                    "type": "string",
                    "default": "",
                    "title": "The author Schema",
                    "examples": [
                        "chris bagel"
                    ]
                },
                "language": {
                    "type": "string",
                    "default": "",
                    "title": "The language Schema",
                    "examples": [
                        "english"
                    ]
                },
                "pages": {
                    "type": "integer",
                    "default": 0,
                    "title": "The pages Schema",
                    "examples": [
                        400
                    ]
                },
                "publisher": {
                    "type": "string",
                    "default": "",
                    "title": "The publisher Schema",
                    "examples": [
                        "cheetah man"
                    ]
                },
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "crooks in tow"
                    ]
                },
                "year": {
                    "type": "integer",
                    "default": 0,
                    "title": "The year Schema",
                    "examples": [
                        1344
                    ]
                }
            },
            "examples": [{
                "isbn": "numbers as text",
                "amazon_url": "i hv no idk",
                "author": "chris bagel",
                "language": "english",
                "pages": 400,
                "publisher": "cheetah man",
                "title": "crooks in tow",
                "year": 1344
            }]
        }
    },
    "examples": [{
        "books": {
            "isbn":"0691161518",
            "amazon_url": "i hv no idk",
            "author": "chris bagel",
            "language": "english",
            "pages": 400,
            "publisher": "cheetah man",
            "title": "crooks in tow",
            "year": 1344
        }
    }]
}